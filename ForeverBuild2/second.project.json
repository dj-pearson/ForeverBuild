{
  "name": "ForeverBuild",
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "shared": {
        "$path": "src/shared"
      }
    },
    "ServerScriptService": {
      "$className": "ServerScriptService",
      "server": {
        "$path": "src/server"
      }
    },
    "StarterGui": {
      "$className": "StarterGui",
      "StarterGui": {
        "$path": "src/StarterGui"
      }
    },
    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "client": {
          "$path": "src/client"
        }
      }
    }
  },  "exclude": [
    "**/*.meta.json",
    "src/server/init.lua",
    "src/server/init.luau",
    "src/server/init.server.lua",
    "src/server/init.server.luau",
    "src/client/init.lua",
    "src/client/init.luau",
    "src/client/init.client.lua",
    "src/client/init.client.luau"
  ]
}